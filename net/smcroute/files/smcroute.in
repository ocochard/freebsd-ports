#!/bin/sh

# PROVIDE: smcroute
# REQUIRE: netif routing
# KEYWORD: nojail

#
# Add the following to /etc/rc.conf[.local] to enable this service
#
# smcroute_enable="YES"
#

. /etc/rc.subr

name="smcroute"
rcvar="${name}_enable"
command="%%PREFIX%%/sbin/${name}d"

load_rc_config $name

: ${smcroute_enable="NO"}
: ${smcroute_flags=""}

run_rc_command "$1"
